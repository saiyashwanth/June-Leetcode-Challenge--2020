


   class Solution {
    public int hIndex(int[] citations) {
        int left=0,right=citations.length-1,mid=0,n=citations.length;
        while(left<=right){

        mid=left+(right-left)/2;
            if(citations[mid]<n-mid)left=mid+1;
            else
                right=mid-1;
        }
        return n-left;
    }
}
